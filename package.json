{
  "name": "server-todo",
  "version": "0.0.1",
  "description": "Project for learn NodeJS and BD.",
  "main": "app/index.js",
  "scripts": {
    "test": "jest && exit 1",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "precommit": "npm run lint",
    "lint": "eslint app/**/*.js"
  },
  "betterScripts": {
    "start": {
      "command": "node app/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon ./app/index.js --experimental-worker",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "author": "Slardarius",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-eslint": "^10.0.3",
    "better-npm-run": "^0.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.3",
    "mongoose": "^5.7.11"
  }
